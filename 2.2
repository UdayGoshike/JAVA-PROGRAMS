class vehicle{
    int age=80;
    void start(){
        System.out.println("vehicle is starting..." );
    }
}
class car extends vehicle{
    int age=90;
    void show(){
        System.out.println("car is starting ...");
    }
    void details() {
        System.out.println(age);
        System.out.println(super.age);
    }
}

public class Main {
