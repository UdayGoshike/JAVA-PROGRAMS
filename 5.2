interface implements1 {
    public void run();
    public void show();
}
abstract class implements2{
    abstract  void meth();
    public void conc(){
        System.out.println("concrete method....");
    }
}
class mulinh extends implements2 implements implements1 {
    @Override
    public void run() {
        System.out.println("running...");
    }
    @Override
    public void show() {
        System.out.println("showing....");
    }

    @Override
    public void meth(){
        System.out.println("Method1 running.............");
    }

}

public class Main {
    public static void main(String[] args) {
        mulinh obj1=new mulinh();
        obj1.run();
        obj1.show();
        obj1.conc();
        obj1.meth();
    }
}

